{
    "name": "YANT",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "Yet Another New Tab",
    "homepage_url": "https://github.com/Thomaash/yant",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "chrome_url_overrides": {
        "newtab": "src/newtab/index.html"
    },
    "permissions": [
        "storage"
    ]
}
